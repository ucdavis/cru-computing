{
    {% for post in site.categories.web_proj %}
    "{{post.title}}": {
        "title": "{{post.title}}",
        "description": "{{post.description | strip_html | strip_newlines }}",
        "client": "{{post.client}}",
        {% if post.client-email %}
        "client-email": "{{post.client-email}}",
        {% endif %}
        "current-url": "{{post.current-url}}"
        {% if post.backend %}
        ,"backend": "{{post.backend}}"
        {% endif %}
        {% if post.Launched %}
        ,"launched": "{{post.Launched}}"
        {% endif %}
    } {% unless forloop.last %},{% endunless %}
    {% endfor %}
}